{
  "name": "mcp-doctor",
  "version": "0.1.0",
  "description": "Automated repair system for Model Context Protocol (MCP) servers",
  "main": "dist/index.js",
  "scripts": {
    "start": "electron .",
    "dev": "tsc && electron .",
    "build": "tsc",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "jest",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "postinstall": "electron-builder install-app-deps"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ghenghis/mcp-doctor.git"
  },
  "keywords": [
    "MCP",
    "Model",
    "Context",
    "Protocol",
    "Claude",
    "Windsurf",
    "Cursor",
    "repair",
    "diagnostics"
  ],
  "author": "Ghenghis",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Ghenghis/mcp-doctor/issues"
  },
  "homepage": "https://github.com/Ghenghis/mcp-doctor#readme",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.11.0",
    "electron-log": "^4.4.8",
    "electron-updater": "^5.3.0",
    "fs-extra": "^11.2.0",
    "glob": "^8.1.0",
    "lodash": "^4.17.21",
    "node-fetch": "^2.7.0",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.4",
    "@types/lodash": "^4.14.200",
    "@types/node": "^18.17.11",
    "@types/node-fetch": "^2.6.9",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "electron": "^23.1.3",
    "electron-builder": "^24.6.4",
    "eslint": "^8.46.0",
    "jest": "^29.6.4",
    "prettier": "^2.8.8",
    "ts-jest": "^29.1.1",
    "typescript": "^5.1.6"
  }
}